<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_2_5-0.dtd">
<sun-web-app error-url="">
  <context-root>/hotelapp-war</context-root>
  <resource-ref>
    <res-ref-name>jdbc/onlinehotel_MySQL</res-ref-name>
    <jndi-name>jdbc/onlinehotel_MySQL</jndi-name>
  </resource-ref>
  <resource-ref>
    <res-ref-name>jms/reservationmessageFactory</res-ref-name>
    <jndi-name>jms/reservationmessageFactory</jndi-name>
  </resource-ref>
  <message-destination-ref>
    <message-destination-ref-name>jms/reservationmessage</message-destination-ref-name>
    <jndi-name>jms/reservationmessage</jndi-name>
  </message-destination-ref>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</sun-web-app>
